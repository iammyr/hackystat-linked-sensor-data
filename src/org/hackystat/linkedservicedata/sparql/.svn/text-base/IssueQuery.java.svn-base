package org.hackystat.linkedservicedata.sparql;

/**
 * Utility class to group all the issue features as filters for Sparql query on Issues.
 *
 * @author Myriam Leggieri.
 *
 */
public class IssueQuery {
  public String id = null;
  public String type = null;
  public String created = null;
  public String closed = null;
  public String lastmod = null;
  public String[] tags = null;
  public String[] duplicates = null;
  public String[] sameAs = null;
  public String[] seeAlso = null;
  public String assignedTo = null;
  public String status = null;
  public String priority = null;
  public String milestone = null;

}
